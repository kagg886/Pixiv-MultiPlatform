headers = gif_rust.h
staticLibraries = libgif_rust.a
linkerOpts.ios_arm64 = -Bstatic -Lsrc/rust/target/aarch64-apple-ios/release/ -lgif_rust
linkerOpts.ios_x64 = -Bstatic -Lsrc/rust/target/x86_64-apple-ios/release/ -lgif_rust
linkerOpts.ios_simulator_arm64 = -Bstatic -Lsrc/rust/target/aarch64-apple-ios-sim/release/ -lgif_rust
libraryPaths.ios_arm64 = src/rust/target/aarch64-apple-ios/release/
libraryPaths.ios_x64 = src/rust/target/x86_64-apple-ios/release/
libraryPaths.ios_simulator_arm64 = src/rust/target/aarch64-apple-ios-sim/release/
